  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "pdfjs-dist": "4.2.67"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.66",
    "react": "^18.2.0",
    "typescript": "^5.2.2",
    "tsup": "^8.0.2",
    "glob": "^10.4.1"
